   "source": [
    "def count_vowels(word):\n",
    "    vowels = \"AEIOUaeiou\"\n",
    "    counts = 0\n",
    "    for char in word:\n",
    "        if char in vowels:\n",
    "            counts += 1\n",
    "    return counts\n",
    "\n",
    "def main():\n",
    "    print(count_vowels(\"Tandon\"))\n",
    "\n",
    "main()"
   ]
